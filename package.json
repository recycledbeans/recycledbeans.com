{
  "name": "mattw.me",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "cross-env": "^7.0.2",
    "laravel-mix": "^5.0.4",
    "pygments": "https://github.com/dracula/pygments.git",
    "pygments-css": "https://github.com/richleland/pygments-css.git",
    "tailwindcss": "^1.3.4",
    "vue": "^2.6.11"
  },
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
